{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "implicit-arrow-linebreak": "off",
    "consistent-return": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "quotes": "off",
    "comma-dangle": ["error", "only-multiline"]
  }
}
